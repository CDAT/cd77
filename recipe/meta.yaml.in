package:
    name: cd77
    version: 1.1

source:
    git_rev: @UVCDAT_BRANCH@
    git_url: git://github.com/cdat/cd77.git

build:
  number: @BUILD_NUMBER@
  script: python setup.py install
 
requirements:
  build:
    - python
    - setuptools
  run:
    - libcdms
    - libdrs_f
    - ezget
    - lats
    - python
    - gcc_linux-64  # [linux]
    - gfortran_linux-64  # [linux]
    - clang_osx-64  # [osx]
    - gfortran_osx-64  # [osx]

about:
    home:  http://github.com/cdat/cd77

